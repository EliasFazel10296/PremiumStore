<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:id="@+id/rippleMask"
        android:width="55dp"
        android:height="55dp" >

        <ripple
            android:color="#00A5FF">

            <item android:id="@android:id/mask">

                <shape android:shape="oval">

                    <solid android:color="#00A5FF" />

                </shape>

            </item>

        </ripple>

    </item>

    <item
        android:height="55dp"
        android:width="55dp">

        <shape
            android:shape="oval">

            <solid android:color="#237DAE"/>

        </shape>

    </item>

    <item
        android:height="55dp"
        android:width="55dp"
        android:drawable="@drawable/contact_icon">

    </item>

</layer-list>