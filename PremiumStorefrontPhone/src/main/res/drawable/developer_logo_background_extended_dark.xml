<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- Top Right -->
    <item
        android:height="57dp"
        android:width="57dp"
        android:gravity="top|end">

        <shape
            android:shape="rectangle">

            <solid
                android:color="@color/premiumDark"/>

        </shape>

    </item>

    <item
        android:id="@+id/clearLayer"
        android:height="57dp"
        android:width="57dp"
        android:gravity="top|end">

        <shape
            android:shape="rectangle">

            <corners
                android:bottomRightRadius="51dp"/>

            <solid
                android:color="@color/green"/>

        </shape>

    </item>

</layer-list>