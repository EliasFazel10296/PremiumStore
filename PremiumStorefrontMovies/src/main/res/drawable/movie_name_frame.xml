<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:id="@+id/strokeLayer"
        android:top="23dp"
        android:bottom="23dp"
        android:left="23dp"
        android:right="23dp"
        android:gravity="top|center_horizontal">

        <shape
            android:shape="rectangle">

            <corners
                android:radius="23dp"/>

            <stroke
                android:color="@color/default_color"
                android:width="3dp"
                android:dashWidth="13dp"
                android:dashGap="7dp"/>

        </shape>

    </item>

</layer-list>